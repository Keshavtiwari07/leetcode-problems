class Solution {
public:
    int numberOfEmployeesWhoMetTarget(vector<int>& hours, int target) {
        int ans = 0;
        for(int i = 0; i < hours.size(); i++){ 
            if(hours[i] >= target){ // if employee met the target
                ans++; // increase the ans by 1
            }
            
        }
        return ans;
    }
};